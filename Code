		//计算5!
		int c = 5;
		int rel = 1;
		while(c > 0) {
			rel *= c;
			c--;
		}
		System.out.println(rel);

		// 5! + 4! + 3! + 2! + 1！
		int d = 1;
		int num = 0;
		int result = 1;
		while(d <= 5) {
			result *= d;
			d++;
			num += result;
		}
		System.out.println(num);

	}

